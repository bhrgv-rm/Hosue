{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.7.4"
    }
  },
  "routes": [
    {
      "src": "^/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "^/(.*\\.php)$",
      "dest": "/api/index.php"
    },
    {
      "src": "^/$",
      "dest": "/api/index.php"
    }
  ]
}